{
    "id": "mod-graphql-1.2.1",
    "name": "GraphQL API for FOLIO",
    "provides": [
        {
            "id": "graphql",
            "version": "1.0.0",
            "handlers": [
                {
                    "methods": [
                        "POST"
                    ],
                    "pathPattern": "/graphql",
                    "permissionsRequired": []
                }
            ]
        }
    ],
    "launchDescriptor": {
        "dockerImage": "mod-graphql:0.1.0",
        "dockerPull": true,
        "dockerArgs": {
            "HostConfig": {
                "Memory": 402653184,
                "PortBindings": {
                    "3001/tcp": [
                        {
                            "HostPort": "%p"
                        }
                    ]
                }
            }
        }
    }
}