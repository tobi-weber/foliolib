{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Collection of users",
    "UserCollection": {
        "type": "object",
        "properties": {
            "users": {
                "description": "List of userdata items",
                "type": "array",
                "items": {
                    "$ref": "user.json#/User"
                }
            },
            "totalRecords": {
                "type": "integer"
            },
            "resultInfo": {
                "$ref": "../common/resultInfo.json#/ResultInfo",
                "readonly": true
            }
        },
        "required": [
            "users",
            "totalRecords"
        ]
    }
}