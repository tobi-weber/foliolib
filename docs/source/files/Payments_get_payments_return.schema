{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Collection of payments",
  "type": "object",
  "properties": {
    "payments": {
      "description": "List of payment items",
      "type": "array",
      "id": "paymentsData",
      "items": {
        "type": "object",
        "$ref": "paymentdata.json"
      }
    },
    "totalRecords": {
      "type": "integer"
    },
    "resultInfo": {
      "$ref": "raml-util/schemas/resultInfo.schema",
      "readonly": true
    }
  },
  "required": [
    "payments",
    "totalRecords"
  ]
}